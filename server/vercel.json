{
  "version": 2,
  "functions": {
    "src/**/*.ts": {
      "includeFiles": [
        "node_modules/pdfjs-dist/legacy/build/pdf.min.mjs",
        "node_modules/pdfjs-dist/build/pdf.worker.min.js"
      ]
    }
  },
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}